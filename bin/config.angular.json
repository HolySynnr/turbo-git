{
  "git-log-command": "git log -n50 --reverse",
  "turbo-log-parsing-limit": 50,
  "turbo-log-extras": {},
  "commit-convention": [
    {
      "tag": "feat",
      "desc": "features commits.",
      "color": "green"
    },
    {
      "tag": "fix",
      "desc": "bug fix commits.",
      "color": "red"
    },
    {
      "tag": "docs",
      "desc": "documentation commits",
      "color": "blue"
    },
    {
      "tag": "style",
      "desc": "formatting, missing semi colons.",
      "color": "yellow"
    },
    {
      "tag": "refactor",
      "desc": "refactor commits.",
      "color": "cyan"
    },
    {
      "tag": "test",
      "desc": "when adding missing tests.",
      "color": "white"
    },
    {
      "tag": "chore",
      "desc": "maintain commits",
      "color": "magenta"
    }
  ],
  "debug": false
}
